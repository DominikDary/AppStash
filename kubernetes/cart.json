{
  "id": "cart",
  "kind": "ReplicationController",
  "apiVersion": "v1beta1",
  "desiredState": {
    "replicas": 2,
    "replicaSelector": {"name": "cart"},
    "podTemplate": {
      "desiredState": {
        "manifest": {
          "version": "v1beta1",
          "id": "cart",
          "containers": [
            {
              "name": "cart",
              "image": "zutherb/cart-service",
              "cpu": 100,
              "memory": 50000000,
              "ports": [
                {
                  "containerPort": 18100,
                  "hostPort": 18100
                }
              ]
            }
          ]
        }
      },
      "labels": {
        "name": "cart",
        "uses": "redis"
      }
    }
  },
  "labels": {"name": "cart"}
}