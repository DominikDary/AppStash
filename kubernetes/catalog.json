{
  "id": "catalog",
  "kind": "ReplicationController",
  "apiVersion": "v1beta1",
  "desiredState": {
    "replicas": 2,
    "replicaSelector": {"name": "frontend"},
    "podTemplate": {
      "desiredState": {
        "manifest": {
          "version": "v1beta1",
          "id": "catalog",
          "containers": [
            {
              "name": "catalog",
              "image": "zutherb/catalog-frontend",
              "cpu": 100,
              "memory": 50000000,
              "ports": [
                {
                  "containerPort": 80,
                  "hostPort": 80
                }
              ]
            }
          ]
        }
      },
      "labels": {
        "name": "catalog",
        "uses": "product,navigation,cart,shop"
      }
    }
  },
  "labels": {"name": "catalog"}
}